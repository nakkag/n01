IDR_MENU_MAIN MENU DISCARDABLE 
BEGIN
    POPUP "ゲーム(&G)"
    BEGIN
        MENUITEM "新規ゲーム(&N)...",           ID_MENUITEM_NEW_GAME
        MENUITEM SEPARATOR
        MENUITEM "新規スケジュール(&E)...",     ID_MENUITEM_NEW_SCHEDULE
        MENUITEM "スケジュール変更(&M)...",     ID_MENUITEM_SET_SCHEDULE
        MENUITEM SEPARATOR
        MENUITEM "ゲーム履歴(&H)...",           ID_MENUITEM_GAME_HISTORY
        MENUITEM SEPARATOR
        MENUITEM "オプション(&O)...",           ID_MENUITEM_OPTION
        MENUITEM "スコアを自動保存(&S)",        ID_MENUITEM_AUTO_SAVE
        MENUITEM "チェックアウトモード(&C)",    ID_MENUITEM_CHECK_OUT_MODE
        MENUITEM SEPARATOR
        MENUITEM "n01 オンライン対戦(&L)",      ID_MENUITEM_ONLINE
        MENUITEM SEPARATOR
        MENUITEM "終了(&X)",                    ID_MENUITEM_EXIT
    END
    POPUP "表示(&V)"
    BEGIN
        MENUITEM "名前(&N)",                    ID_MENUITEM_SHOW_NAME
        MENUITEM "プレイヤー情報(&P)",          ID_MENUITEM_SHOW_PLAYER
        MENUITEM "残りスコア(&L)",              ID_MENUITEM_SHOW_LEFT
        MENUITEM "ファンクション キー(&K)",     ID_MENUITEM_SHOW_GUIDE
        MENUITEM SEPARATOR
        POPUP "残りスコアのサイズ(&S)"
        BEGIN
            MENUITEM "大(&L)",                      ID_MENUITEM_FONT_L
            MENUITEM "中(&M)",                      ID_MENUITEM_FONT_M
            MENUITEM "小(&S)",                      ID_MENUITEM_FONT_S
        END
        MENUITEM "フルスクリーン(&F)",          ID_MENUITEM_FULL_SCREEN
        MENUITEM SEPARATOR
        MENUITEM "インデックスをダーツ数で表示(&I)", 
                                                ID_MENUITEM_SHOW_DARTSCOUNT
    END
    POPUP "スコア(&S)"
    BEGIN
        MENUITEM "&1本目で終了",                ID_MENUITEM_FINISH_ONE
        MENUITEM "&2本目で終了",                ID_MENUITEM_FINISH_TWO
        MENUITEM "&3本目で終了",                ID_MENUITEM_FINISH_THREE
        MENUITEM SEPARATOR
        MENUITEM "ミドル フォー ディドル(&M)...", ID_MENUITEM_MIDDLE
        MENUITEM SEPARATOR
        POPUP "定型入力(&I)"
        BEGIN
            MENUITEM "(なし)",                      ID_MENUITEM_NOTHING
            , GRAYED
        END
        MENUITEM "残りスコアでの入力(&L)",      ID_MENUITEM_SCORE_LEFT
        MENUITEM SEPARATOR
        MENUITEM "前のレッグに戻る(&P)",        ID_MENUITEM_PREV_LEG
        MENUITEM "スコアの入れ替え(&W)",        ID_MENUITEM_SCORE_SWAP
        MENUITEM "スコアのクリア(&C)",          ID_MENUITEM_SCORE_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "アレンジの表示(&A)...",       ID_MENUITEM_ARRANGE
    END
    POPUP "ヘルプ(&H)"
    BEGIN
        MENUITEM "バージョン情報(&A)...",       ID_MENUITEM_ABOUT
    END
END


